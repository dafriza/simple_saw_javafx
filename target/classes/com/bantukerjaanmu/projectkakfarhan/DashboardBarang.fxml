<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bantukerjaanmu.projectkakfarhan.DashboardBarangController">
   <children>
      <TableView fx:id="barangTable" layoutX="24.0" layoutY="123.0" onMouseClicked="#clickRow" prefHeight="251.0" prefWidth="553.0">
        <columns>
          <TableColumn fx:id="colId" prefWidth="41.0" text="No" />
          <TableColumn fx:id="colBarang" prefWidth="109.0" text="Barang" />
            <TableColumn fx:id="colGroup" prefWidth="119.0" text="Grup" />
            <TableColumn fx:id="colTanggal" prefWidth="283.0" text="Tanggal" />
        </columns>
      </TableView>
      <Label layoutX="24.0" layoutY="13.0" prefHeight="35.0" prefWidth="329.0" text="Dashboard Barang">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <HBox alignment="BOTTOM_LEFT" layoutX="24.0" layoutY="45.0" prefHeight="31.0" prefWidth="553.0" spacing="10.0">
         <children>
            <Button id="newDecision" fx:id="newProductButton" mnemonicParsing="false" onAction="#switchToAddProduct" text="New Product" />
            <Button id="newDecision" fx:id="editButton" mnemonicParsing="false" onAction="#switchToEditProduct" text="Edit" />
            <Button id="newDecision" fx:id="deleteButton" mnemonicParsing="false" onAction="#delete" text="Delete" />
            <Button id="newDecision" fx:id="backButton" mnemonicParsing="false" onAction="#switchToDashboard" text="Back" />
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" layoutX="24.0" layoutY="75.0" prefHeight="45.0" prefWidth="338.0" spacing="10.0">
         <children>
            <Label alignment="BOTTOM_LEFT" text="Search" />
            <TextField promptText="Search ,,," />
         </children>
      </HBox>
   </children>
</AnchorPane>
